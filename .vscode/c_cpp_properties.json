{
    "env": {
        "myDefaultIncludePath": [
            "${workspaceFolder}/**",
            "${env:CONDAHOME}/include"
        ]
    },
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${myDefaultIncludePath}"
            ],
            "defines": [],
            "compilerPath": "${env:AMSHOME}/tools/cdsgcc/gcc/bin/gcc",
            "intelliSenseMode": "gcc-x64",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}